<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"] - Korona Takip</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <link rel="icon" href="~/images/ERBAY LOGO.png">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="it">
    <meta name="keywords" content="creative, agency, startup,onepage, clean, modern,business, company,it,mavzer,mavzerbay,net,asp">

    <meta name="author" content="mavzerbaynet">

    <!-- bootstrap.min css -->
    <link href="~/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animate Css -->
    <link href="~/plugins/animate-css/animate.css" rel="stylesheet" />
    <!-- Icon Font css -->
    <link href="~/plugins/fontawesome/css/all.css" rel="stylesheet" />
    <link href="~/plugins/fonts/Pe-icon-7-stroke.css" rel="stylesheet" />
    <!-- Themify icon Css -->
    <link href="~/plugins/themify/css/themify-icons.css" rel="stylesheet" />
    <!-- Slick Carousel CSS -->
    <link href="~/plugins/slick-carousel/slick/slick.css" rel="stylesheet" />
    <link href="~/plugins/slick-carousel/slick/slick-theme.css" rel="stylesheet" />

    <!-- Main Stylesheet -->
    <link href="~/css/style.css" rel="stylesheet" />
</head>
<body id="top-@Context.Request.Host@Context.Request.Path">
    <!-- LOADER TEMPLATE -->
    @*<div id="page-loader">
            <div class="loader-icon fa fa-spin colored-border"></div>
        </div>*@
    <!-- /LOADER TEMPLATE -->

    @{
        Html.RenderPartialAsync("Menu");
    }


    
            @RenderBody()
       



    @{ 
        Html.RenderPartialAsync("Footer");
    }
    <!--  Page Scroll to Top  -->

    <a class="scroll-to-top" href="#top-@Context.Request.Host@Context.Request.Path">
        <i class="fa fa-angle-up"></i>
    </a>
    <!--
    Essential Scripts
    =====================================-->
    <!-- Main jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 3.1 -->
    <script src="~/plugins/bootstrap/js/popper.min.js"></script>
    <script src="~/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Slick Slider -->
    <script src="~/plugins/slick-carousel/slick/slick.min.js"></script>
    <script src="~/plugins/jquery/jquery.easing.1.3.js"></script>
    <!-- Map Js -->
    <script src="~/plugins/google-map/gmap3.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDwIQh7LGryQdDDi-A603lR8NqiF3R_ycA"></script>

    <script src="~/plugins/form/contact.js"></script>
    <script src="~/js/theme.js"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
